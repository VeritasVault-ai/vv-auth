{
  "name": "@veritasvault/vv-auth",
  "version": "0.1.0",
  "description": "Authentication library for VeritasVault services",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware.js",
      "require": "./dist/middleware.cjs",
      "types": "./dist/middleware.d.ts"
    },
    "./compliance": {
      "import": "./dist/compliance.js",
      "require": "./dist/compliance.cjs",
      "types": "./dist/compliance.d.ts"
    },
    "./wallet": {
      "import": "./dist/wallet.js",
      "require": "./dist/wallet.cjs",
      "types": "./dist/wallet.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "docs": "typedoc --out docs src"
  },
  "keywords": [
    "auth",
    "authentication",
    "defi",
    "web3",
    "wallet",
    "supabase",
    "nextauth",
    "veritasvault"
  ],
  "author": "VeritasVault",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/VeritasVault-ai/vv-auth.git"
  },
  "dependencies": {
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.45.0",
    "@web3modal/wagmi": "^4.0.11",
    "ethers": "^6.11.1",
    "next-auth": "^4.24.7",
    "rxjs": "^7.8.2",
    "viem": "^2.9.4",
    "wagmi": "^2.5.12",
    "web3": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.9",
    "@types/react": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.5",
    "vite": "^5.2.2",
    "vite-plugin-dts": "^3.8.1",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "next": ">=15.0.0",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
